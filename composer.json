{
  "name": "plaisio/request-logger-light",
  "description": "PhpPlaisio: Request Logger for light websites and development",
  "keywords": [
    "PhpPlaisio",
    "Request Logger"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-mbstring": "*",
    "plaisio/company-resolver": "^2.1.0",
    "plaisio/kernel": "^3.2.1",
    "plaisio/request": "^0.13.1",
    "plaisio/request-handler": "^2.1.0",
    "plaisio/request-logger": "^1.3.0",
    "plaisio/session": "^5.0.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0-RC4",
    "plaisio/console-kernel": "^1.0.1",
    "setbased/php-stratum-mysql": "^6.6.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\RequestLogger\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
