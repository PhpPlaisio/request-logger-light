{
  "name": "plaisio/request-logger-light",
  "description": "PhpPlaisio: Request Logger for light websites and development",
  "keywords": [
    "PhpPlaisio",
    "Request Logger"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "ext-mbstring": "*",
    "plaisio/kernel": "^1.1",
    "plaisio/request-logger": "^1.1"
  },
  "require-dev": {
    "phing/phing": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\RequestLogger\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
