{
  "name": "plaisio/request-logger-light",
  "description": "PhpPlaisio: Request Logger for light websites and development",
  "keywords": [
    "PhpPlaisio",
    "Request Logger"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.3",
    "ext-mbstring": "*",
    "plaisio/company-resolver": "^2.2.0",
    "plaisio/kernel": "^3.3.1",
    "plaisio/request": "^2.0.0",
    "plaisio/request-handler": "^2.2.0",
    "plaisio/request-logger": "^1.4.0",
    "plaisio/session": "^5.1.0",
    "setbased/php-stratum-mysql": "^7.2.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.1",
    "plaisio/console-kernel": "^1.2.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\RequestLogger\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
